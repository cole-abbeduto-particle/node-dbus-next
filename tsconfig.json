{
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "noEmit": true,
    "maxNodeModuleJsDepth": 2,
    "allowJs": true,
    "checkJs": false,
    "resolveJsonModule": true
  }
}
